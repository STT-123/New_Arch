    "version": "0.2.0",
    "configurations": [
        {
            "name": "bat_ecu",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/bat_ecu",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath":"/usr/bin/gdb-multiarch",//"miDebuggerPath":"gdb-multiarch",
            "miDebuggerServerAddress":"192.168.1.110:2001",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}